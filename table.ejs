    <body style="margin: 10vh;">
        <table class="table table-bordered">
            <thead>
                <% header.forEach(head => { %>
                    <th><%= head %> </th>
                <% }); %> 
            </thead>
            <tbody>
                <% rows.forEach(row => { %>
                    <tr>
                        <% row.forEach(element => { %>
                            <th><%= element %> </th>
                        <% }) %> 
                    </tr>
                <% }); %> 
            </tbody>
        </table>
        <hr>
    </body>
</html>