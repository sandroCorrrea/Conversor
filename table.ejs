<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>Titulo</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' type='text/css' media='screen' href='#'>
    <script src='#'></script>
</head>
<body>
    <table>
        <thead>
            <% header.forEach(element => { %>
                <tr>
                    <th><%= element %> </th>
                </tr>
            <% }) %> 
        </thead>
        <tbody>
            <% rows.forEach(row => { %>
                <tr>
                    <% row.forEach(element => { %>
                        <th><%= element %> </th>
                    <% }) %> 
                </tr>
            <% }) %> 
        </tbody>
    </table>
</body>
</html>